db.collection.aggregate([
  {
    $addFields: {
      ipOnly: {
        $regexFind: {
          input: "$fieldName", // Replace with your field name
          regex: "^(?:\\[?)([0-9a-fA-F.:]+)(?:\\]?)(?::\\d+)?$" // Captures IP, excludes port
        }
      }
    }
  },
  {
    $group: {
      _id: "$ipOnly.match" // Group by extracted IP
    }
  },
  {
    $match: {
      "_id": { $ne: null } // Exclude null matches
    }
  },
  {
    $project: {
      _id: 0,
      ip: "$_id"
    }
  }
])
